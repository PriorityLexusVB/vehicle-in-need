{
  "name": "pre-order-dealer-exchange-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "prebuild:check": "node scripts/check-conflicts.cjs",
    "prebuild": "npm run prebuild:check",
    "build": "vite build",
    "postbuild": "bash scripts/verify-css-in-build.sh",
    "preview": "vite preview",
    "start": "node server/index.cjs",
    "server": "node server/index.cjs",
    "audit:ui": "bash scripts/ui-audit.sh",
    "audit:bundle": "npm run build && command -v source-map-explorer >/dev/null 2>&1 && source-map-explorer dist/assets/*.js || echo '⚠️  source-map-explorer not installed (npm i -g source-map-explorer)'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:functions": "cd functions && npm test",
    "test:rules": "firebase emulators:exec --only firestore 'vitest --config vitest.rules.config.ts --run'",
    "test:rules:watch": "firebase emulators:exec --only firestore 'vitest --config vitest.rules.config.ts'",
    "verify:rules": "bash scripts/verify-firestore-rules.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "lint:md:fix": "markdownlint-cli2 --fix \"**/*.md\" \"#node_modules\"",
    "lint:cloudbuild": "bash scripts/check-cloudbuild-service-url.sh",
    "cloudbuild:list-triggers": "bash scripts/list-cloud-build-triggers.sh",
    "cloudbuild:verify-trigger": "bash scripts/verify-cloud-build-config.sh",
    "cloudbuild:diagnose": "bash scripts/diagnose-cloud-build-error.sh",
    "cloudbuild:setup-iam": "bash scripts/setup-iam-permissions.sh",
    "verify:parity": "node scripts/verify-deploy-parity.cjs https://pre-order-dealer-exchange-tracker-842946218691.us-west1.run.app/",
    "verify:css": "bash scripts/verify-css-in-build.sh",
    "verify:production": "bash scripts/verify-production-state.sh",
    "verify:version": "node scripts/check-production-version.cjs",
    "verify:deployment": "bash scripts/post-deployment-verification.sh",
    "sync:production": "bash scripts/sync-production-with-main.sh",
    "predeploy": "bash scripts/pre-deploy-css-check.sh",
    "deploy:functions": "cd functions && npm run build && cd .. && firebase deploy --only functions --project vehicles-in-need",
    "deploy:functions:dry-run": "cd functions && npm run build && cd .. && firebase deploy --only functions --project vehicles-in-need --dry-run",
    "seed:managers:dry-run": "node scripts/seed-manager.mjs --project vehicles-in-need --dry-run",
    "seed:managers:apply": "node scripts/seed-manager.mjs --project vehicles-in-need --apply",
    "seed:options:dry-run": "node scripts/seed-vehicle-options.mjs --project vehicles-in-need --dry-run",
    "seed:options:apply": "node scripts/seed-vehicle-options.mjs --project vehicles-in-need"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "firebase": "^12.6.0",
    "firebase-admin": "^13.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^6.30.2",
    "vite-plugin-node-polyfills": "^0.24.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.4",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.25",
    "firebase-tools": "^14.27.0",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "markdownlint-cli2": "^0.20.0",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.3.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.15",
    "workbox-window": "^7.3.0"
  },
  "packageManager": "npm@10.8.2"
}
