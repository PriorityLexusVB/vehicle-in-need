{
  "name": "vehicle-order-tracker-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for Vehicle Order Tracker",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "deploy": "firebase deploy --only functions",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "firebase-admin": "^13.0.2",
    "firebase-functions": "^6.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.4",
    "firebase-functions-test": "^3.4.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.9.3"
  },
  "private": true
}
